<div id="<%= dom_id course %>">
  <p>
    <strong>Start at:</strong>
    <%= course.start_at %>
  </p>

  <p>
    <strong>End at:</strong>
    <%= course.end_at %>
  </p>

  <p>
    <strong>Week day:</strong>
    <%= course.week_day %>
  </p>

  <p>
    <strong>Classe:</strong>
    <%= course.classe_id %>
  </p>

  <p>
    <strong>Subject:</strong>
    <%= course.subject_id %>
  </p>

  <p>
    <strong>Moment:</strong>
    <%= course.moment_id %>
  </p>

  <p>
    <strong>Person:</strong>
    <%= course.person_id %>
  </p>

</div>
